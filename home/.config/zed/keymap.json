[
  {
    "context": "Editor && vim_mode == normal && vim_operator == none && !VimWaiting",
    "bindings": {
      "space f": "editor::Format",
      "cmd-/": [
        "editor::ToggleComments",
        {
          "advance_downwards": false
        }
      ],
      "space p o": "projects::OpenRecent",
      "space p f": "file_finder::Toggle",
      "space p s": "workspace::NewSearch"
    }
  },
  {
    "context": "Editor && vim_mode == visual && vim_operator == none && !VimWaiting",
    "bindings": {
      "shift-j": "editor::MoveLineDown",
      "shift-k": "editor::MoveLineUp"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-shift-tab": "pane::ActivatePreviousItem",
      "ctrl-tab": "pane::ActivateNextItem",
      "ctrl-cmd-p": null,
      "cmd-shift-d": "editor::DuplicateLineDown",
      "ctrl-cmd-up": "editor::MoveLineUp",
      "ctrl-cmd-down": "editor::MoveLineDown",
      "pageup": "editor::MovePageUp",
      "pagedown": "editor::MovePageDown"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "cmd-1": [
        "pane::ActivateItem",
        0
      ],
      "cmd-2": [
        "pane::ActivateItem",
        1
      ],
      "cmd-3": [
        "pane::ActivateItem",
        2
      ],
      "cmd-4": [
        "pane::ActivateItem",
        3
      ],
      "cmd-5": [
        "pane::ActivateItem",
        4
      ],
      "cmd-6": [
        "pane::ActivateItem",
        5
      ],
      "cmd-7": [
        "pane::ActivateItem",
        6
      ],
      "cmd-8": [
        "pane::ActivateItem",
        7
      ],
      "cmd-9": "pane::ActivateLastItem"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      // TODO: fix, "zed::NoAction" doesn't seem to work to override this
      // keybinding, so we're overriding with a random keybinding that
      // doesn't affect the editor...
      // "cmd-shift-e": null,
      "ctrl-cmd-p": [
        "projects::OpenRecent",
        {
          "create_new_window": false
        }
      ]
    }
  },
  {
    "context": "ProjectPanel && not_editing",
    "bindings": {
      "/": null
    }
  }
]
